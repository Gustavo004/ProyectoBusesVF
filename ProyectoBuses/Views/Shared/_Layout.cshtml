                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Marcas", "Index", "Marca", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("Sucursales", "Index", "Sucursal", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("Clientes", "Index", "Cliente", new { area = "" }, new { @class = "navbar-brand" })
